<div class="wrapper">
	<%= render 'dashboard_shared/sidenavigation' %>
	<div class="main-panel">
		<%= render 'dashboard_shared/navigation_bar' %>

		<div class="content">
			<div class="container-fluid">
				<div class="row">
					<div class="col-md-12">
						<h1 class="h2">Orders</h1>
                        <div class="content table-responsive table-full-width">
                        	<table class="table table-hover table-striped">
                        		<thead>

                        			<th>Date</th>
                        			<th>State</th>
                        			<th>Description</th>
                        			<th>Shipping Address</th>
                        			<th>Phone</th>
															<th>Qty</th>
                        		</thead>
                        		<tbody>
                        			<% @orders.each do |f| %>
                        				<tr>
                        					<td><%= f.created_at.strftime("%B %d, %Y, %T") %></td>
                        					<td><%= f.state %></td>
                        					<td><%= f.description %></td>
                        					<td><%= f.shipping_address %></td>
                        					<td><%= f.phone %></td>
																	<td><%=f.quantity%>
                        				</tr>
                        			<% end %>
                        		</tbody>
                        	</table>
                        </div>
					</div>
				</div>
			</div>
		</div>

	</div>
</div>


